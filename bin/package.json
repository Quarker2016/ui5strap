{
  "name": "ui5strap-bin",
  "version": "0.0.1",
  "author": "Jan Philipp Knoeller",
  "license": "Apache2",
  "scripts": {
    "run-web": "node ../server/server.js",
    "run-desktop": "nwbuild -r ../www",
    "run-android": "cd .. && cordova emulate android",
    
    "build-ui5strap-themes": "grunt --gruntfile ../build/pks.ui5strap/themes/build-ui5strap-themes.js",
    
    "clean-ui5strap-library": "grunt cleanup --gruntfile ../build/pks.ui5strap/library/build-ui5strap-library.js",
    "build-ui5strap-library": "grunt --gruntfile ../build/pks.ui5strap/library/build-ui5strap-library.js",
  	
  	"build-ui5strap-docs" : "jsdoc ../www/lib/ui5strap -r -d ../docs",
  	
    "build-demoapp-component": "grunt --gruntfile build-demoapp-preload.js"
    
  },
  "devDependencies": {
    "cordova": "*",
    "grunt": "*",
    "grunt-cli": "*",
    "grunt-contrib-concat": "*",
    "grunt-contrib-uglify": "*",
    "grunt-openui5": "*",
    "http-server": "*",
    "jsdoc": "*",
    "nw-builder": "*"
  }
}
