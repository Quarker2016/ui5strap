{
        "app" : {
            "name" : "Ui5Strap Boilerplate",
            "package" : "com_mycompany.my_app",
            "title" : "Ui5Strap",
            "logLevel" : 0
        },

        "developers" : [
            {
                "name" : "Jan Philipp KnÃ¶ller",
                "email" : "info@pksoftware.de",
                "timezone" : -1,
                "roles" : [
                    "Developer"
                ]
            }
        ],

        "licenses" : [
            {
                "name" : "Apache2 License",
                "url" : "http://www.apache.org/licenses/LICENSE-2.0.txt"
            }
        ],

        "libraries" : [ 
            {
                "package" : "ui5strap",
                "location" : "./lib/ui5strap/"
            },
            {
                "package" : "my_lib",
                "location" : "./lib/my_lib/"
            }
        ],

        "css" : [
            {
                "id" : "bootstrap-css",
                "package" : "ui5strap",
                "src" : "bootstrap-3.1.1-dist/css/bootstrap.min.css"
            },
            {
                "package" : "ui5strap",
                "src" : "font-awesome-4.1.0/css/font-awesome.min.css"
            },
            {
                "package" : "my_lib",
                "src" : "css/clock.css"
            },
            "css/my-app.css"
        ],

        "models" : [
            {
                "src" : "i18n/i18n.properties",
                "type" : "RESOURCE",
                "modelName" : "i18n" 
            },
            {
                "src" : "data/data-example.json",
                "type" : "JSON",
                "modelName" : "data" 
            }
        ],

        "frames" : {
            "default" : {
                "module" : "com_mycompany.my_app.modules.MyFrameController",
                "navContainer" : "ui5strap.NavContainerStandard",
                "initialViews" : [
                    { 
                        "viewName" : "com_mycompany.my_app.views.Home"
                    }
                ],
                "navbarMenu" : "mainMenu"
            }
        },

        "menus" : {
            "mainMenu" : { 
                "items" : [
                    {
                        "viewName" : "com_mycompany.my_app.views.Installation",
                        "label" : "i18n>MENU_INSTALLATION"
                    },
                    {
                        "sidebarMenu" : "configurationMenu",
                        "label" : "i18n>MENU_CONFIGURATION"
                    },
                    {
                        "viewName" : "com_mycompany.my_app.views.Controls",
                        "label" : "i18n>MENU_CONTROLS"
                    },
                    {
                        "viewName" : "com_mycompany.my_app.views.Support",
                        "label" : "i18n>MENU_SUPPORT"
                    }
                ]
            },

            "configurationMenu" : {
                "parent" : "mainMenu",
                "items" : [
                        {
                            "viewName" : "com_mycompany.my_app.views.configuration.Configuration",
                            "label" : "i18n>MENU_CONFIG_GENERAL"
                        },
                        {
                            "viewName" : "com_mycompany.my_app.views.configuration.Theme",
                            "label" : "i18n>MENU_CONFIG_THEME"
                        },
                        {
                            "viewName" : "com_mycompany.my_app.views.configuration.Frame",
                            "label" : "i18n>MENU_CONFIG_FRAME"
                        }
                ]
            }
        },

        "views" : {
            "com_mycompany.my_app.views.Home" : {
                "id" : "my-app-home",
                "target" : "content",
                "type" : "HTML",
                "navbar" : true
            },

            "com_mycompany.my_app.views.Installation" : {
                "id" : "my-app-installation",
                "target" : "content",
                "type" : "HTML",
                "navbar" : true
            },
            
            "com_mycompany.my_app.views.Controls" : {
                "id" : "my-app-controls",
                "target" : "content",
                "type" : "HTML",
                "showLoader" : true,
                "navbar" : true
            },
            
            "com_mycompany.my_app.views.configuration.Theme" : {
                "id" : "my-app-config-theme",
                "target" : "content",
                "type" : "HTML",
                "sidebar" : true,
                "navbar" : true,
                "sidebarMenu" : "configurationMenu"
            },

            "com_mycompany.my_app.views.configuration.Frame" : {
                "id" : "my-app-config-frame",
                "target" : "content",
                "type" : "HTML",
                "sidebar" : true,
                "navbar" : true,
                "sidebarMenu" : "configurationMenu"
            },

            "com_mycompany.my_app.views.configuration.Configuration" : {
                "id" : "my-app-configuration",
                "target" : "content",
                "type" : "HTML",
                "sidebar" : true,
                "navbar" : true,
                "sidebarMenu" : "configurationMenu"
            },
            
            "com_mycompany.my_app.views.Support" : {
                "id" : "my-app-support",
                "target" : "content",
                "type" : "HTML",
                "navbar" : true
            }
        }
}