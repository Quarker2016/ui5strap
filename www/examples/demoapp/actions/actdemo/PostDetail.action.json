{
    "BEGIN" : "readPost",
    
    "readPost" : {
        "thenCallback" : "= then()",
        
        "feedManager" : "= app.getFeedManager()",
        
        "DO" : "= .feedManager.readPost(event.parameters.post.id, .thenCallback)",
        
        "THEN" : "setModel"
    },
    
    "setModel" : {
        "TYPE" : "pks.ui5strap.action.AMSetModel",
        "modelName" : "POST",
        "data" : "= action.readPost.callbackArgs.0"
    }


}