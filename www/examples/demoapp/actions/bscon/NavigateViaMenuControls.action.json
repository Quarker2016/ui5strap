{
	"//" : "Navigates to the page selected on the Controls section menu.",
	
	"BEGIN" : "navigate",
	
	"navigate" : {
		"TYPE" : "pks.ui5strap.action.NavigateTask",
		
		"modelName" : "menu",
		"contextData" : "= event.parameters.listItem.getBindingContextData(.modelName)",
		
		"CONTROLS" : {
			"NAVIGATOR" : {
				"TYPE" : "pks.ui5strap.viewer.NavContainer",
				"SOURCE" : "View",
				"CONTROL_ID" : "subNavControls",
				"VIEW_ID" : "controls"
			}
		},
		
		"VIEWS" : {
			"menuTarget" : {
				"id" : "= .contextData.id",
				"transition" : "zoom-out"
			}
		}
	}

}