{
	
	"TASKS" : ["getContextData", "closeOverlay"],
	
	"getContextData" : {
	    "TYPE" : "pks.ui5strap.action.AMGetContextData",
		"modelName" : "tasks",
		"tgtParam" : "parameters.getContextData.CONTEXT",
		"scope" : "SOURCE"
	},

	"closeOverlay" : {
	    "TYPE" : "pks.ui5strap.action.AMCloseOverlay",
		"scope" : "VIEWER", 
		"THEN" : "showApp"
	},

	"openApp" : { 
		"target" : "VIEWER" 
	},
	
	"showApp" : {
	   "TYPE" : "pks.ui5strap.action.AMShowApp",
	   "id" : "= action.getContextData.CONTEXT.app.id"
	}

}