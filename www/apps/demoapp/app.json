{
	"app" : {
	        "id" : "ui5strap.demoapp",
	        "package" : "ui5strap.demoapp",
	        "name" : "Ui5Strap Demo",
	        "logLevel" : 5
	},
	
	"models" : [
            {
                "src" : "i18n/i18n.properties",
                "type" : "RESOURCE",
                "modelName" : "i18n" 
            },
            {
                "src" : "data/menu.json",
                "type" : "JSON",
                "modelName" : "menu" 
            },
            {
                "src" : "data/posts.json",
                "type" : "JSON",
                "modelName" : "posts" 
            }
    ],
    
    "views" : {
    		"ui5strap.demoapp.views.MenuRoot" : {
                "type" : "HTML",
                
                "id" : "my-app-bar-menu",
                
                "target" : "bar",
                "preload" : true
            },
            
            "ui5strap.demoapp.views.Home" : {
                "type" : "HTML",
                
                "id" : "home",
                
                "target" : "content",
                "preload" : true
            },

            "ui5strap.demoapp.views.MyPage2" : {
                "type" : "HTML",
                
                "id" : "my-app-page2",
                
                "target" : "content",
                "preload" : true
            },

            "ui5strap.demoapp.views.MyPage3" : {
                "type" : "HTML",
                
                "id" : "my-app-page3",
                
                "target" : "content",
                "preload" : true
            },

            "ui5strap.demoapp.views.MyPage4" : {
                "type" : "HTML",
                
                "id" : "my-app-page4",
                
                "target" : "content",
                "preload" : true,
                
                "events" : {
                	"controller" : {
                		"update" : [
                			"ui5strap.demoapp.actions.LoadMenuSub1",
                			"ui5strap.demoapp.actions.NavigateToPage5"
                		]
                	}
                }
            },
			
			"ui5strap.demoapp.views.MenuSub1" : {
                "type" : "HTML",
                
                "id" : "my-app-bar-sub-menu",
                
                "target" : "bar",
                "preload" : true
            },
            
            "ui5strap.demoapp.views.MyPage5" : {
                "type" : "HTML",
                
                "id" : "my-app-page5",
                
                "target" : "content",
                "preload" : true
            },
            
            "ui5strap.demoapp.views.MyPage6" : {
                "type" : "HTML",
                
                "id" : "my-app-page6",
                
                "target" : "content",
                "preload" : true
            }
      },
	
	"components" : [
	         {
	             
	             "id" : "frame",
	             "module" : "ui5strap.AppFrame",
	             "navContainer" : "ui5strap.BarNavContainer",
	             "navContainerOptions" : {
	             	"barPlacementExtraSmall" : "Top",
	             
	             	"barSizeSmall" : 2,
	             	"barPlacementSmall" : "Right",
	             	
	             	"barSizeMedium" : 2,
	             	"barPlacementMedium" : "Bottom",
	             	"barTransitionMedigum" : "none",
	             	
	             	"barSizeLarge" : 2,
	             	"barPlacementLarge" : "Left",
	             	"barModeLarge" : "IntrudeFixed",
	             	
	             	"barVisible" : false
	             	
	             },
	             
	             "events" : {
	             	"control" : {
	             		"pageShow" : [
	             			"ui5strap.demoapp.actions.SetSelectedMenuRoot"
	             		]
	             	}
	             },
	             
	             "initialViews" : [
	                 { 
	                     "viewName" : "ui5strap.demoapp.views.Home",
	                     "type" : "HTML"
	                 },
	                 { 
	                     "viewName" : "ui5strap.demoapp.views.MenuRoot",
	                     "type" : "HTML"
	                 }
	             ]
	
	         }
    ]
}