{
	"app" : {
	        "id" : "ui5strap.demoapp",
	        "package" : "ui5strap.demoapp",
	        "name" : "My First App",
	        "logLevel" : 5
	},
	
	"models" : [
            {
                "src" : "i18n/i18n.properties",
                "type" : "RESOURCE",
                "modelName" : "i18n" 
            },
            {
                "src" : "data/menu.json",
                "type" : "JSON",
                "modelName" : "menu" 
            }
    ],
    
    "views" : {
            "ui5strap.demoapp.views.MyPage1" : {
                "type" : "HTML",
                
                "id" : "my-app-page1",
                "styleClass" : "page-1",

                "target" : "content",
                "preload" : true
            },

            "ui5strap.demoapp.views.MyPage2" : {
                "type" : "HTML",
                
                "id" : "my-app-page2",
                "styleClass" : "page-2",

                "target" : "content",
                "preload" : true
            },

            "ui5strap.demoapp.views.MyPage3" : {
                "type" : "HTML",
                
                "id" : "my-app-page3",
                "styleClass" : "page-3",

                "target" : "content",
                "preload" : true
            }
      },
	
	"components" : [
	         {
	             
	             "id" : "frame",
	             "module" : "ui5strap.AppFrame",
	             "navContainer" : "ui5strap.BarNavContainer",
	             "navContainerOptions" : {
	             	"placement" : "Left",
	             	"barVisible" : false,
	             	"barMode" : "Intrude"
	             },
	             
	             "initialViews" : [
	                 { 
	                     "viewName" : "ui5strap.demoapp.views.MyPage1",
	                     "type" : "HTML"
	                 }
	             ]
	
	         }
    ]
}