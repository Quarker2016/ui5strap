{
	"app" : {
	        "id" : "ui5strap.demoapp",
	        "package" : "ui5strap.demoapp",
	        "name" : "Ui5Strap Demo",
	        "logLevel" : 5
	},
	
	"css" : [
		"css/demoapp.css"
	],
	
	"models" : [
            {
                "src" : "i18n/i18n.properties",
                "type" : "RESOURCE",
                "modelName" : "i18n" 
            },
            {
                "src" : "data/menu.json",
                "type" : "JSON",
                "modelName" : "menu" 
            }
    ],
    
    "views" : {
    		"ui5strap.demoapp.views.MenuRoot" : {
                "type" : "XML",
                
                "id" : "my-app-bar-menu",
                
                "target" : "bar"
            },
            
            "ui5strap.demoapp.views.Home" : {
                "type" : "XML",
                
                "id" : "home"
            },

            "ui5strap.demoapp.views.Feed" : {
                "type" : "XML",
                
                "id" : "feed"
            },

            "ui5strap.demoapp.views.MyPage3" : {
                "type" : "XML",
                
                "id" : "my-app-page3"
            },

            "ui5strap.demoapp.views.Settings" : {
                "type" : "XML",
                
                "id" : "settings",
                
                "events" : {
                	"controller" : {
                		"update" : [
                			"ui5strap.demoapp.actions.LoadMenuSettings",
                			"ui5strap.demoapp.actions.NavigateToSettingsLanguage"
                		]
                	}
                }
            },
            
            "ui5strap.demoapp.views.MenuSettings" : {
                "type" : "XML",
                
                "id" : "menuSettings",
                
                "target" : "bar"
            },
            
            "ui5strap.demoapp.views.settings.Language" : {
                "type" : "XML",
                
                "id" : "settingsLanguage",
                
                "events" : {
                	"controller" : {
                		"update" : "ui5strap.demoapp.actions.SetCurrentLanguage"	
                	}
                }
            },
            
            "ui5strap.demoapp.views.settings.Theme" : {
                "type" : "XML",
                
                "id" : "settingsTheme"
            },
            
            
            "ui5strap.demoapp.views.Controls" :{
            	"id" : "controls",
            	"type" : "XML",
                
                "events" : {
                	"controller" : {
                		"update" : [
                			"ui5strap.demoapp.actions.LoadMenuControls",
                			"ui5strap.demoapp.actions.NavigateToControlsButtons"
                		]
                	}
                }
            },
            
            "ui5strap.demoapp.views.MenuControls" : {
                "type" : "XML",
                
                "id" : "menuControls",
                
                "target" : "bar"
            },
            
            "ui5strap.demoapp.views.controls.Buttons" :{
            	"id" : "controlsButtons",
            	"type" : "HTML"
            },
            
            "ui5strap.demoapp.views.controls.Containers" :{
            	"id" : "controlsContainers",
            	"type" : "HTML"
            },
            
            "ui5strap.demoapp.views.controls.Icons" :{
            	"id" : "controlsIcons",
            	"type" : "HTML"
            },
            
            "ui5strap.demoapp.views.controls.Typography" :{
            	"id" : "controlsTypography",
            	"type" : "HTML"
            },
			
			"ui5strap.demoapp.views.controls.Navs" :{
            	"id" : "controlsNavs",
            	"type" : "HTML"
            },
            
            "ui5strap.demoapp.views.controls.GridSystem" :{
            	"id" : "controlsGridSystem",
            	"type" : "HTML"
            },
            
            "ui5strap.demoapp.views.controls.Progress" :{
            	"id" : "controlsProgress",
            	"type" : "HTML"
            },
            
            "ui5strap.demoapp.views.controls.Form" :{
            	"id" : "controlsForm",
            	"type" : "HTML"
            },
            
            "ui5strap.demoapp.views.controls.Indicators" :{
            	"id" : "controlsIndicators",
            	"type" : "HTML"
            },
            
            "ui5strap.demoapp.views.controls.MediaList" :{
            	"id" : "controlsMediaList",
            	"type" : "HTML"
            },
            
            "ui5strap.demoapp.views.controls.Dialogs" :{
            	"id" : "controlsDialogs",
            	"type" : "HTML"
            },
            
            "ui5strap.demoapp.views.NewPost" : {
                "type" : "XML",
                
                "id" : "my-app-new-post"
            }
      },
	
	"actions" : [
		"ui5strap.demoapp.actions.NavigateToHome",
		"ui5strap.demoapp.actions.NavigateToFeed",
		"ui5strap.demoapp.actions.NavigateToPage3",
		
		
		"ui5strap.demoapp.actions.ToggleBarMenuRoot",
		"ui5strap.demoapp.actions.NavigateViaMenuRoot",
		"ui5strap.demoapp.actions.SetSelectedMenuRoot",
		
		"ui5strap.demoapp.actions.LoadMenuSettings",
		"ui5strap.demoapp.actions.ToggleBarMenuSettings",
		"ui5strap.demoapp.actions.NavigateViaMenuSettings", 
		"ui5strap.demoapp.actions.SetSelectedMenuSettings",
		"ui5strap.demoapp.actions.NavigateToSettingsLanguage",
		
		"ui5strap.demoapp.actions.LoadMenuControls",
		"ui5strap.demoapp.actions.ToggleBarMenuControls",
		"ui5strap.demoapp.actions.NavigateViaMenuControls", 
		"ui5strap.demoapp.actions.SetSelectedMenuControls",
		"ui5strap.demoapp.actions.NavigateToControlsButtons",
		
		"ui5strap.demoapp.actions.AppendFeed",
		"ui5strap.demoapp.actions.RemovePost",
		"ui5strap.demoapp.actions.ShowNewPostOverlay",
		"ui5strap.demoapp.actions.CloseOverlay",
		
		"ui5strap.demoapp.actions.ChangeTheme",
		"ui5strap.demoapp.actions.SelectLanguage",
		
		"ui5strap.demoapp.actions.Debug"
	],
	
	"components" : [
	         {
	             
	             "id" : "frame",
	             "module" : "ui5strap.AppFrame",
	             "navContainer" : "ui5strap.BarNavContainer",
	             "navContainerOptions" : {
	             	"barSizeExtraSmall" : 1,
	             	"barPlacementExtraSmall" : "Top",
	             	
	             
	             	"barSizeSmall" : 2,
	             	"barPlacementSmall" : "Right",
	             	
	             	"barSizeMedium" : 1,
	             	"barPlacementMedium" : "Bottom",
	             	"barTransitionMedigum" : "none",
	             	
	             	"barSizeLarge" : 2,
	             	"barPlacementLarge" : "Left",
	             	"barModeLarge" : "IntrudeFixed",
	             	
	             	"barVisible" : false
	             	
	             },
	             
	             "events" : {
	             	"control" : {
	             		"pageChange" : [
	             			"ui5strap.demoapp.actions.SetSelectedMenuRoot"
	             		]
	             	}
	             },
	             
	             "initialViews" : [
	                 { 
	                     "viewName" : "ui5strap.demoapp.views.Home"
	                 },
	                 { 
	                     "viewName" : "ui5strap.demoapp.views.MenuRoot"
	                 }
	             ]
	
	         },
	         
	         {
	             
	             "id" : "feedClient",
	             "module" : "ui5strap.RestClient",
	             "url" : "/apps/demoapp/server/feed",
	             "methods" : {
	             	"info" : {
	             		"path" : "info",
	             		"type" : "get",
	             		"arguments" : ["success", "error"]
	             	}
	             }
	         }
    ]
}