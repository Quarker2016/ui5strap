{
	"//" : "Set the sub-menu item selected that has the same id as specified NavContainer's current page.",
	"__format" : "AJ2.0",
	
	"__modules" : [
		"ui5strap.AMGetCurrentPage", 
		"ui5strap.AMSetListItemSelected"
	],
	
	"getCurrentPage" : {
		"scope" : "SOURCE",
		"target" : "content"
	},
	
	"setListItemSelected" : {
		"itemId" : "=parameters.getCurrentPage.result",
		"controlId" : "mySubMenu",
		"viewId" : "my-app-bar-sub-menu",
		"scope" : "APP"
	}
}