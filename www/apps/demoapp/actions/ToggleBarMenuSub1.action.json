{
	"//" : "Toggles the visibility of the menu bar of the root NavContainer.",
	"__format" : "AJ2.0",
	
	"__modules" : "ui5strap.task.Call",
	
	"call" : {
		"controlId" : "subNav",
		"viewId" : "my-app-page4",
		"navContainer" : "&=> app.getControl(.controlId, .viewId)",
		"currentValue" : "&=> .navContainer.getBarVisible()",
		"oppositeValue" : "&=> tools.not(.currentValue)",
		"RESULT" : "&=> .navContainer.setBarVisible(.oppositeValue)"
	}

}