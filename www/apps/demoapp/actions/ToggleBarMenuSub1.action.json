{
	"//" : "Toggles the visibility of the menu bar of the specified NavContainer.",
	
	"TASKS" : "toggleBarMenuSub1",
	
	"toggleBarMenuSub1" : {
		"TASK" : "ui5strap.task.Exec",
		
		"controlId" : "subNav",
		"viewId" : "my-app-page4",
		"navContainer" : "&=> app.getControl(.controlId, .viewId)",
		
		"currentValue" : "&=> .navContainer.getBarVisible()",
		"oppositeValue" : "&=> tools.bool.not(.currentValue)",
		
		"RESULT" : "&=> .navContainer.setBarVisible(.oppositeValue)"
	}

}