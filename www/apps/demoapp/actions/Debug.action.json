{
	"//" : "Shows a alert window with the content of the specified custom data field.",
	"TASKS" : "testTask",
	
	"testTask" : {
		"TASK" : "ui5strap.task.Exec",
		
		"compareValue" : "DELETE",
		"customDataKey" : "command",
		"customDataValue" : "&=> eventParameters.srcControl.data(.customDataKey)",
		
		"IF" : "&=> tools.lang.eq(.compareValue, .customDataValue)",
		
		"message" : "&=> .customDataValue",
		"RESULT" : "&=> window.alert(.message)",
		
		"ELSE" : "error"
	},
	
	"error" : {
		"TASK" : "ui5strap.task.Exec",
		
		"message" : "Error occurred.",
		
		"RESULT" : "&=> window.alert(.message)"
	}
	
}