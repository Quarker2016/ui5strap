{
	"TASKS" : "changeTheme",

	"changeTheme" : {
		"TYPE" : "ui5strap.AMChangeTheme",
		
		"CONTROLS" : {
			"themeButton" : {
				"mode" : "Event",
				"type" : "ui5strap.Button",
				"parameter" : "button"
			}
		},
		
		
		"customDataKey" : "theme",
		"theme" : "= .CONTROLS.themeButton.data(.customDataKey)"
	}
	
}