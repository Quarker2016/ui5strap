{
	"//" : "Makes the specified NavContainer navigate to the view that is specified in the list item context of the specified model.",
	
	"__modules" : ["ui5strap.AMGetContextData", "ui5strap.task.Navigate"],
	
	"getContextData" : {
		"scope" : "PARAMETER",
		"tgtParam" : "parameters.menuItem",
		"modelName" : "menu",
		"parameterKey" : "listItem"
	},
	
	"navigate" : {
		"controlId" : "subNav",
		"viewId" : "my-app-page4",
		"CONTROL" : "&=> app.getControl(.controlId, .viewId)",
		"PAGE" : {
			"transition" : "zoom-out",
			"writeHistory" : true,
			"viewName" : "&=> parameters.menuItem.viewName",
			"target" : "content"
		}
	}

}