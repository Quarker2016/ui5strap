{
	"__format" : "AJ2.0",
	"__modules" : ["ui5strap.AMGetCurrentPage", "ui5strap.AMSetListItemSelected"],
	"__description" : "Set the main-menu item selected that has the same id as specified NavContainer's current page.",
	
	"getCurrentPage" : {
		"scope" : "SOURCE",
		
		"target" : "content"
	},
	
	"setListItemSelected" : {
		"scope" : "APP",
		"viewId" : "my-app-bar-menu",
		"controlId" : "myMenu",
		
		"itemId" : "&=> pool.getCurrentPage.result"
	}
}