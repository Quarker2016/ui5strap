{
	"//" : "Set the main-menu item selected that has the same id as specified NavContainer's current page.",
	
	"__modules" : ["ui5strap.AMGetCurrentPage", "ui5strap.AMSetListItemSelected"],
	
	"getCurrentPage" : {
		"scope" : "SOURCE",
		
		"target" : "content"
	},
	
	"setListItemSelected" : {
		"scope" : "APP",
		"viewId" : "my-app-bar-menu",
		"controlId" : "myMenu",
		
		"itemId" : "&=> action.getCurrentPage.result"
	}
}