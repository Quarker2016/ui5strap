{
	"//" : "Toggles the visibility of the menu bar of the root NavContainer.",
	"__format" : "AJ2.0",
	
	"__modules" : "ui5strap.task.Call",
	
	"call" : {
		"currentValue" : "= .subject.control.getBarVisible()",
		"oppositeValue" : "= tools.not(.currentValue)",
		"method" : "= .subject.control.setBarVisible(.oppositeValue)"
	}

}