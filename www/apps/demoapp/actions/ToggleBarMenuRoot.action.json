{
	"//" : "Toggles the visibility of the menu bar of the root NavContainer.",
	"__format" : "AJ2.0",
	
	"__modules" : "ui5strap.task.Exec",
	
	"exec" : {
		"navContainer" : "&=> app.getRootControl()",
		"currentValue" : "&=> .navContainer.getBarVisible()",
		"oppositeValue" : "&=> tools.bool.not(.currentValue)",
		"RESULT" : "&=> .navContainer.setBarVisible(.oppositeValue)"
	}

}