{
	"//" : "Makes the specified NavContainer navigate to the view that is specified in the list item context of the specified model.",
	
	"__modules" : ["ui5strap.AMGetContextData", "ui5strap.task.Navigate"],
	
	"getContextData" : {
		"scope" : "PARAMETER",
		"tgtParam" : "parameters.menuItem",
		"modelName" : "menu",
		"parameterKey" : "listItem"
	},
	
	"navigate" : {
		"CONTROLS" : {
			"navContainer" : {
				"TYPE" : "ui5strap.NavContainer",
				"SOURCE" : "View",
				"CONTROL_ID" : "subNavControls",
				"VIEW_ID" : "controls"
			}
		},
		
		"VIEWS" : {
			"menuTarget" : {
				"transition" : "zoom-out",
				"writeHistory" : true,
				"id" : "= parameters.menuItem.id",
				"target" : "content"
			}
		}
	}

}