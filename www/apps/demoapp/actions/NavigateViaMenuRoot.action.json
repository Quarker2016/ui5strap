{
	"//" : "Makes the root NavContainer navigate to the view that is specified in the list item context of the specified model.",
	
	"__modules" : ["ui5strap.AMGetContextData", "ui5strap.task.Navigate"],
	
	"getContextData" : {
		"scope" : "PARAMETER",
		"tgtParam" : "action.menuItem",
		"modelName" : "menu",
		"parameterKey" : "listItem"
	},
	
	"navigate" : {
		"PAGE" : {
			"viewName" : "=-> action.menuItem.viewName",
			"transition" : "flip-horizontal-ccw",
			"writeHistory" : true
		}
	}

}