{
	
	"__format" : "AJ2.0",
	"__modules" : [
		{
			"module" : "ui5strap.AMGetProperty",
			"namespace" : "getTitle"
		},
		{
			"module" : "ui5strap.AMGetProperty",
			"namespace" : "getMessage"
		},
		{
			"module" : "ui5strap.AMCreateControl",
			"namespace" : "createMediaListItem"
		},
		{
			"module" : "ui5strap.AMCreateControl",
			"namespace" : "createMediaListImage"
		},
		{ 
			"module" : "ui5strap.AMSetAggregation",
			"namespace" : "addImageToItem"
		}, 
		{ 
			"module" : "ui5strap.AMAddAggregation",
			"namespace" : "addItemToList"
		}
	],
	"__description" : "Appends a new media list item to an existing media list.",
	
	"getTitle" : {
		"scope" : "VIEW",
		"controlId" : "titleInput",
		
		"propertyName" : "value",
		"tgtParam" : "parameters.createMediaListItem.settings.heading"
	},
	
	"getMessage" : {
		"scope" : "VIEW",
		"controlId" : "messageInput",
		
		"propertyName" : "value",
		"tgtParam" : "parameters.createMediaListItem.settings.text"
	},
	
	"createMediaListItem" : {
		"moduleName" : "ui5strap.ListMediaItem"
	},
	
	"createMediaListImage" : {
		"moduleName" : "ui5strap.Image",
		"settings" : {
			"type" : "MediaObject",
			"mpath" : "ui5strap.demoapp.img.awesome",
			"ext" : "png",
			"width":64
		}
	},
	
	"addImageToItem" : {
		"parameterKey" : "parameters.createMediaListItem.result",
		"scope" : "CONTEXT",
		
		"aggregation" : "media",
		"instance" : "=parameters.createMediaListImage.result"
	},
	
	"addItemToList" : {
		"controlId" : "feeds",
		"scope" : "VIEW",
		
		"aggregation" : "items",
		"instance" : "=parameters.createMediaListItem.result"
	}

}